https://leetcode.com/problems/move-zeroes/submissions/

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
    //check whether your ith pos is zero or not if not then continue checking for the rest elements if found zero then swap it
    int j=0;
    for(int i=0;i<nums.size();i++)
    {
       if(nums[i]!=0)
       swap(nums[i],nums[j++]);
       else 
       continue;
    }
    }
};
