
https://leetcode.com/problems/subsets/

class Solution {
public:
    //subsets of set is similar to subset of string here we are taking two vectors ip and op vectors
    void solve(vector<int>ip, vector<int>op,vector<vector<int>>&ans){
        if(ip.size() == 0){
            ans.push_back(op);   
            return;
        }
        vector<int>op1 = op;
        vector<int>op2 = op;
        
        op2.push_back(ip[0]);
        ip.erase(ip.begin() + 0);
        
        solve(ip, op1,ans);
        solve(ip, op2,ans); 
        return;
    }
    vector<vector<int>> subsets(vector<int>& nums) {
        vector<vector<int>>ans;
        vector<int>op;
        vector<int>ip=nums;
        solve(ip,op,ans);
        return ans; 
    }
};
